{
  "name": "@felixtz/iwsdk-rag-mcp",
  "version": "0.2.0",
  "description": "MCP server providing semantic code search and API reference for the Immersive Web SDK using RAG (Retrieval-Augmented Generation) with vector embeddings",
  "main": "dist/index.js",
  "bin": {
    "iwsdk-rag-mcp": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "build:tools": "tsc -p tsconfig.tools.json",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "node test/compare_results.js",
    "prepare": "npm run build",
    "ingest": "npm run build:tools && node dist-tools/tools/ingest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "iwsdk",
    "immersive-web-sdk",
    "webxr",
    "rag",
    "retrieval-augmented-generation",
    "code-search",
    "semantic-search",
    "vector-embeddings",
    "api-reference",
    "claude",
    "ai-assistant"
  ],
  "author": {
    "name": "Felix Zhang",
    "email": "felix@elixr.games",
    "url": "https://github.com/felixtrz"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/felixtrz/iwsdk-rag-mcp"
  },
  "bugs": {
    "url": "https://github.com/felixtrz/iwsdk-rag-mcp/issues"
  },
  "homepage": "https://github.com/felixtrz/iwsdk-rag-mcp#readme",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/felixtrz"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.1.2",
    "@langchain/core": "^1.0.5",
    "@langchain/textsplitters": "^1.0.0",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "glob": "^12.0.0",
    "ts-morph": "^27.0.2",
    "webgpu": "^0.3.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "data",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.19.0"
  },
  "packageManager": "pnpm@10.15.1"
}
